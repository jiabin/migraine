#!/usr/bin/env php
<?php

require __DIR__.'/../vendor/autoload.php';

use Migraine\Command;
use Migraine\Migraine;

$application = new Migraine('Migraine', '@package_version@');
$application->add(new Command\SelfUpdateCommand());
$application->add(new Command\MigrateCommand());
$application->add(new Command\CreateCommand());
$application->run();
